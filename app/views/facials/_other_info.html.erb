</div>
<div class="aldbH2_box">
	<ul class="aldb_ul" id="aldb_ul">
		<%@cases.each do |ca|%>
		<li>
			<div class="aldb_border">
				<div class="beforeimg">
					<%=image_tag ca.before_image,width:150,height:180%>
					<span class="tr1s"><a href="#">before</a></span> 
				</div>
				<div class="afterimg">
					<%=image_tag ca.after_image,width:150,height:180%> <span class="tr1s"><a href="#">after</a></span> 
				</div>
			</div>
			<h2>
				<%=ca.title%>
			</h2>
		</li>
		<%end%>
		
	</ul>
	<div class="aldb_left">
	</div>
	<div class="aldb_right">
	</div>
</div>
