</div>
<h2 class="yszjalH2">
	项目疑问 Project Question
</h2>
<div class="blank10">
</div>
<ul class="ysxmH2_ul">
	<%@questions.each_with_index do |q,i|%>

	<li>
		<a href="#"><img src="/images/img36.png" width="148" height="108" alt="" /></a> 
		<div>
			<i>0<%=i+1%></i> 
			<h3>
				<%=q.question%>
			</h3>
			<p>
				<%=raw q.answer%>
			</p>
		</div>
	</li>
	<%end%>
	
</ul>
